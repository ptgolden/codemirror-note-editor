{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "comma-dangle": 0,
    "curly": [2, "multi-line"],
    "new-cap": 0,
    "no-mixed-requires": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": [2, "nofunc"],
    "quotes": 0,
    "semi": 0
  }
}
